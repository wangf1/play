<mvc:View controllerName="openui5.tutorial.wt.main.Detail"
	xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:wt="openui5.tutorial.wt.control">
	<Page title="{i18n>detailPageTitle}" showNavButton="true"
		navButtonPress="onNavBack">
		<ObjectHeader intro="{invoice>ShipperName}" title="{invoice>ProductName}" />
		<wt:ProductRating class="sapUiSmallMarginBeginEnd"
			change="onRatingChange" />
	</Page>
</mvc:View>